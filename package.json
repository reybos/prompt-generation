{
    "name": "prompt_generation",
    "version": "1.0.0",
    "description": "Content generation pipeline for children's educational videos",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc && copyfiles \"public/**/*\" dist/ && copyfiles \"src/promts/**/*.txt\" dist/ --up 1",
        "start": "node dist/index.js",
        "generate": "node dist/index.js",
        "help": "node dist/index.js --help",
        "web": "node dist/index.js --web",
        "dev": "nodemon --watch 'src/**/*.ts' --exec \"ts-node src/index.ts --web\"",
        "type-check": "tsc --noEmit",
        "clean": "rimraf dist",
        "demo:fal": "ts-node src/tests/falLlmTest.ts"
    },
    "keywords": [
        "langchain",
        "fal-ai",
        "content-generation",
        "educational",
        "children"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fal-ai/client": "1.5.0",
        "@langchain/core": "0.3.61",
        "cors": "^2.8.5",
        "dotenv": "17.0.1",
        "express": "^4.18.2",
        "langchain": "0.3.29",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "20.19.4",
        "copyfiles": "2.4.1",
        "nodemon": "^3.0.1",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}